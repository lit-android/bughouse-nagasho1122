<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.nickname.myoji.bughouse.launcher">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BugHouse">
        <activity android:name="app.nickname.myoji.bughouse.launcher.LauncherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="app.nickname.myoji.bughouse.count.CountActivity"/>
        <activity android:name="app.nickname.myoji.bughouse.load.LoadActivity"/>
        <activity android:name="app.nickname.myoji.bughouse.save.SaveActivity"/>
        <activity android:name="app.nickname.myoji.bughouse.list.ListActivity"/>
    </application>

</manifest>
